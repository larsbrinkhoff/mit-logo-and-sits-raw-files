  457   (COMMENT BIGRAND EXPR (NDIGITS)
  458    (PROG (NUM)
  459     (SETQ NUM (ADD1 (RANDOM 11)))
  460   L (COND ((LESSP NDIGITS 2)
  461   	 (RETURN NUM)))
  462     (SETQ NUM (PLUS (TIMES NUM 12)î&	BIGRAND EXPR (NDIGITS)                                           RANPRI 105  12/20/77  Page 1.6'
  463   		  (RANDOM 12)))
  464     (SETQ NDIGITS (SUB1 NDIGITS))
  465     (GO L)))

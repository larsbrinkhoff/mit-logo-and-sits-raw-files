	MOVEM A,(S)
	POP P,A
	POPJ P,

REVARG:	MOVE A,[W+WORDF,,DUMMYV]
	MOVEI B,(S)
	CAME A,(B)
	SOJA B,.-1
	MOVEI A,(S)
REVAR1:	ADDI B,1
	CAIG A,(B)
	POPJ P,
	MOVE C,(A)
	EXCH C,(B)
	MOVEM C,(A)
	SOJA A,REVAR1


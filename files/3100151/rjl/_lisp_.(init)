(COMMENT CORE 24)	;TYPING  AT THE ALLOCATOR CAUSES IT TO 
			;GO TO .LISP. (INIT) FOR ALLOCATIONS, INSTEAD OF THE TTY.

(PROG (G H)
	(SETQ H (LIST NIL))
	(UREAD GRIND LISP COM)  
    A	(COND ((NULL ^Q))			;TRAP FOR SELF-LOADING FILES
	      ((EQ H (SETQ G (READ H))))	;THIS SHORT LOOP LOADS IN THE UREAD
	      (T (EVAL G) (GO A)))		;FILE LIKE THE TOP LEVEL LOOP WOULD.
	(SSTATUS CRUNIT DSK RJL)		;HERE'S THE WINNER
	(SETQ BASE(SETQ IBASE 10.))
	(RETURN '(READY)))

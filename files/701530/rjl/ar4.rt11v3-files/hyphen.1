; MAIN HYPHENATION ROUTINE.  UPON ENTRY, R0 CONTAINS
;	THE ADDRESS OF A SINGLE WORD, TERMINATED BY A
;	ZERO BYTE WHICH IS TO BE HYPHENATED.
;"HYPHEN" GOES THROUGH THE WORD FROM RIGHT TO LEFT LOOKING
;FIRST FOR A LEGAL SUFFIX FROM ITS SUFFIX TABLES. WHEN IT FINDS A
;LEGAL PLACE FOR A HYPHEN ACCORDING TO THE TABLE IT SETS THE PARITY
;BIT OF THE CHARACTER THE HYPHEN IS TO BE IN FRONT OF. THEN AFTER
;THAT IS DONE ONCE, IT MAY REPEAT THE OPERATION TREATING THE
;REMAINDER OF THE WORD (WITH THE SUFFIX DROPPED) AS AN ENTIRELY NEW
;ENTITY TO BE HYPHENATED AND LOOKED UP IN THE SUFFIX TABLES. THAT IS,
;IF IT DOES THE RECURSION IT DROPS THE FIRST SUFFIX IT FOUND AND
;PRETENDS THE REMAINDER OF THE WORD HAS A SUFFIX THAT CAN BE LOOKED
;UP IN THE SUFFIX TABLES FOR LEGAL HYPHENATIONS. THIS DOES NOT OCCUR
;IN ALL CASES; IF THE "40" BIT IS SET IN THE FIRST BYTE OF THE
;SUFFIX TABLE ENTRY, IT WILL DO IT, OTHERWISE, NOT.
;
	.GLOBL	HYPHEN,HYPEDF

HYPHEN:
	MOV	R0,WORDP
	INC	WORDP		;POINT TO SECOND CHAR
	TST	HYPEDF
	BNE	HYP06
	TST	HYF
	BEQ	HYP06
	INC	HYPEDF
	CLR	NHYPH
HYP01:
	JSR	PC,PUNCT	;IS THE CHAR. A PUNCT. MARK?
	BNE	HYP02		;NO
	INC	R0		;SKIP OVER IT
	BR	HYP01		;AND RE-LOOP
HYP02:
	JSR	PC,ALPH		;IS CHARACTER ALPHABETIC?
	BNE	HYP06		;NO, SO DON'T HYPHENATE
HYP03:
	INC	R0		;SKIP OVER IT
	JSR	PC,ALPH		;FIND END OF
	BEQ	HYP03		;ALPHABETIC STRING
	DEC	R0		;POINT TO LAST CHARACTER
	MOV	R0,HSTART	;SAVE POINTER
HYP04:
	INC	R0		;IS THE FOLLOWING
	TSTB	(R0)		;CHARACTER
	BEQ	HYP05		;THE WORD TERMINATOR??
	JSR	PC,PUNCT	;OR A KNOWN PUNCTUATION?
	BNE	HYP06		;NO, DON'T HYPHENATE
	BR	HYP04		;LOOP UNTIL TERM. FOUND
HYP05:
	MOV	HSTART,R0	;COMPUTE LENGTH OF WORD
	SUB	WORDP,R0	; (LENGTH-2 REALLY)
	CMP	R0,#<5-2>	;DON'T HYPHENATE 5-LETTER WORDS
	BLE	HYP06		;  OR SHORTER
	JSR	R5,SUFFIX	;GO GET THE SUFFIX FORMS ALLOWED
	JSR	R5,DIGRAM
HYP06:
	BICB	#200,@WORDP	;DISALLOW SINGLE CHAR. BREAK
	RTS	PC		;RETURN TO CALLER
; IS THE CHARACTER UNDER SCAN IN THE CLASS OF KNOWN
;	PUNCTUATION MARKS??
;
PUNCT:
	MOVB	(R0),R2
	CMP	R2,#" /
	BEQ	PU01
	CMP	R2,#'.
	BEQ	PU01
	CMP	R2,#',
	BEQ	PU01
	CMP	R2,#'(
	BEQ	PU01
	CMP	R2,#')
	BEQ	PU01
	CMP	R2,#'"
	BEQ	PU01
	CMP	R2,#'\
	BEQ	PU01
	CMP	R2,#'@
PU01:
	RTS	PC

;
;ROUTINE TO CHECK A CHARACTER FOR UPPER OR LOWER CASE
;	ALPHABETICS
;
ALPH:
	MOVB	(R0),R2
ALPH2:
	CMP	R2,#'A
	BLO	ALP02
	CMP	R2,#'Z
	BLOS	ALP01
	CMP	R2,#A
	BLO	ALP02
	CMP	R2,#Z
	BHI	ALP02
ALP01:
	SEZ
	RTS	PC
ALP02:
	CLZ
	RTS	PC

;
;MAP UPPER CASE TO LOWER CASE
;
MAPLOW:
	CMP	R2,#A
	BHIS	MAP01
	ADD	#A-'A,R2
MAP01:
	RTS	PC

;
;CHECK TO SEE IF CHARACTER IS A VOWEL (Y IS CONSIDERED
;	A VOWEL HERE)
;
VOWEL:
	CMP	R2,#A		;IT CAN BE AN A
	BEQ	VO001
	CMP	R2,#E		;OR E
	BEQ	VO001
	CMP	R2,#I		;OR I
	BEQ	VO001
	CMP	R2,#O		;OR O
	BEQ	VO001
	CMP	R2,#U		;OR U
	BEQ	VO001
	CMP	R2,#Y		;OR EVEN Y
VO001:
	RTS	PC

;
;CHECK TO SEE IF ANY VOWELS PRECEDE THE CURRENT CHARACTER
;	POSITION
;
CHECKVOW:
	MOV	R0,-(SP)
CK01:
	MOVB	-(R0),R2
	JSR	PC,ALPH2
	BNE	CK03
	JSR	PC,MAPLOW	;MAP TO LOWER CASE
	JSR	PC,VOWEL
	BEQ	CK02
	JSR	PC,ALPH
	BEQ	CK01
CK03:	MOV	(SP)+,R0
	CLZ
	RTS	R5
CK02:
	MOV	(SP)+,R0
	SEZ
	RTS	R5

; HYP2 -- SUFFIX AND DIGRAM
;AFTER THE INITIAL HYPHENATION GETS DONE LOOKING FOR LEGAL SUFFIXES,
;WE MAKE A PASS THROUGH DIGRAM. WHAT THIS DOES IS MAKE GUESSES FOR
;GOOD PLACES TO PUT A HYPHEN IN THE ROOT OF THE WORD THAT IS LEFT AFTER
;WE'VE FOUND ALL POSSIBLE HYPHENATION PLACES IN THE SUFFIX. THAT IS,
;IT WORKS ON THE PART OF THE WORD TO THE LEFT OF THE LEFTMOST HYPHEN
;INSERTED BY THE "HYPHEN" ROUTINE. AS AN EXAMPLE OF WHAT IT CONSIDERS
;A "GOOD GUESS", IT TENDS TO LIKE DOUBLE CONSONANTS; BB, FF, ETC.
;IT ALSO HAS PROBABILITIES WORKED OUT FOR OTHER COMBINATIONS AND EACH
;IS ASSIGNED A GOODNESS VALUE. DIGRAM GOES THROUGH AND MUNGS THINGS
;AROUND, THEN COMPARES WHAT IT GETS TO THE VALUE OF "THRESH". IF THE
;GOODNESS RATING IT GETS FOR A POSSIBLE HYPEHANTION POINT IS ABOVE
;THE VLUE OF THRESH, IT ACCEPTS IT. IF NOT, IT THROWS IT AWAY.
;THERE ARE FOUR MAIN DIGRAM TABLES FOR THE CASES OF -XX, XX- WITH
;THE XX AT THE BEGINNING OF THE WORD (NO CHARS PRECEDING), X-X,
;AND XX- WITH OTHER CHARACTERS PRECEDING XX. (XX REPRESENTING ANY TWO
;CHARACTERS.)

DIGRAM:
	MOV	HSTART,R0		;GET PTR TO CHAR PRECEDING LEFTMOST HYPHEN
DIG01:
	JSR	PC,ALPH		;IS CHAR ALPHABETIC?
	BNE	DIG09		;IF NOT, GO AWAY
	JSR	PC,MAPLOW		;MAP UC TO LC
	JSR	PC,VOWEL		;IS CHAR A VOWEL?
	BEQ	DIG02		;IF SO
	DEC	R0		;KEEP GOING
	BR	DIG01		;TILL WE FIND A VOWEL
DIG02:
	MOV	R0,HSTART		;RESET LAST CHAR PTR TO VOWEL
DIG03:
	MOVB	-(R0),R2		;GET CHAR PRECEDING
	JSR	PC,ALPH2		;IS IT ALPHA?
	BNE	DIG09		;IF NOT, GO AWAY
	JSR	PC,MAPLOW		;CHANGE UC TO LC
	JSR	PC,VOWEL		;IS IT A VOWEL?
	BNE	DIG03		;IF NOT, TRY AGAIN
	CLR	MAXDIG		;CLEAR OUR GOODNESS VALUE WORD
	MOV	R0,NHSTART		;SAVE PTR TO 2ND VOWEL
DIG04:
	MOV	#1,MQ		;SET UP FOR FIRST MUNGING
	MOVB	-1(R0),R2		;GET CHAR BEFORE FIRST VOWEL
	JSR	PC,ALPH2		;MAKE SURE OF ALPHA
	BEQ	DIG05		;IF SO
	MOVB	(R0),R2		;GET CURRENT CHAR
	MOV	#A,R1
	JSR	R5,DILOOK		;GET VALUE FACTOR FOR X- WHEN X IS AT BEGINNING OF WORD
	BXH
	BR	DIG07
DIG05:
	MOVB	-2(R0),R2		;GET TWO CHARS BEFORE
	MOV	#XXH,DIG10		;ASSUME CHARS PRECEDE XX
	JSR	PC,ALPH2		;FIND OUT ABOUT THAT
	BEQ	DIG06		;IF SO
	MOV	#BXXH,DIG10		;IF NOT, CORRCT FOR XX AT BEGINNING OF WORD
DIG06:
	MOVB	-1(R0),R1		;GET THE LEFT "X" CHAR
	MOVB	(R0),R2		;NOW THE RIGHT "X" CHAR
	JSR	R5,DILOOK		;MUNG!
DIG10:	XXH		;USING TABLE BXXH OR XXH
DIG07:
	MOVB	(R0)+,R1		;GET VOWEL
	MOVB	(R0),R2		;GET CHAR AFTER VOWEL
	JSR	R5,DILOOK		;MUNG!
	XHX
	MOVB	(R0),R1		;MUNG!
	MOVB	1(R0),R2
	JSR	R5,DILOOK
	HXX
	CMP	MQ,MAXDIG		;SEE IF WE BETTERED OUR BEST
	BLOS	DIG08		;IF NOT, KEEF THE OLD
	MOV	MQ,MAXDIG		;IF SO, SAVE OUR BEST GUESS SO FAR
	MOV	R0,MAXLOC		;SAVE ITS LOCATION
DIG08:
	CMP	R0,HSTART		;SEE IF MORE
	BLO	DIG04		;RUN THAT BY ME ONE MORE TIME
	MOV	NHSTART,HSTART
	CMP	MAXDIG,THRESH		;SEE IF OUR BEST WAS GOOD ENOUGH
	BLO	DIGRAM		;IF NOT, TRY FOR SOMETHING BETTER
	BISB	#200,@MAXLOC		;SAY WE CAN HYPHENATE HERE
	INC	NHYPH		;COUNT HOW MANY HYPHENS
;	MOV	MAXDIG,@OCTBUFP
;	INC	OCTCNT
;	ADD	#2,OCTBUFP
	BR	DIGRAM		;KEEP LOOKING
DIG09:
	RTS	R5
;THIS TAKES TWO CHARS PASSED IN R1 AND R2 AND INDEXES
;INTO THE APPROPRIATE DIGRAM TABLE. IT DOES THIS BY
;BASICALLY DOING:
;
;	(R1*13.)/2 + R2
;
;EACH DIGRAM TABLE (EXCEPT BXH) HAS 26. ROWS OF 26. 4-BIT BYTES EACH.
;SO R1 INDEXES TO THE RIGHT ROW AND R2 INDEXES TO THE RIGHT BYTE
;AND THE REMAINDER OF THE DIVISION BY 2 TELLS YOU IF YOU WANT THE LEFT
;FOUR BITS OR THE RIGHT 4 BITS OF THE BYTE. IN THE FOUR BITS YOU FINALLY
;GET IS A VALUE FROM 0 TO 17.
DILOOK:
	BIC	#-177-1,R2
	BIC	#-177-1,R1
	JSR	PC,MAPLOW
	SUB	#A,R2
	CMP	R2,#Z-A
	BHI	DIL02
	MOV	R2,R3
	MOV	R1,R2
	JSR	PC,MAPLOW
	SUB	#A,R2
	CMP	R2,#Z-A
	BHI	DIL02
	MOV	MQ,-(SP)
	MOV	R2,MQ
	ASL	MQ		; * 2
	ASL	MQ		; * 4
	MOV	MQ,	-(SP)
	ASL	MQ		; * 8
	ADD	(SP)+,	MQ	; * 12
	ADD	R2,	MQ	; * 13
	CLR	R2
	CLC
	ROR	R3
	ADC	R2
	ADD	MQ,R3
	ADD	(R5)+,R3
	MOVB	(R3),R3
	TST	R2
	BNE	DIL01
	ASR	R3
	ASR	R3
	ASR	R3
	ASR	R3
DIL01:
	BIC	#-17-1,R3
	CLR	MQ
	MOV	(SP)+,MUL
	BEQ	20$
10$:	ADD	R3,	MQ
	DEC	MUL
	BNE	10$
20$:	RTS	R5
DIL02:
	CLR	MQ
	TST	(R5)+
	RTS	R5

;
;MARK ANY LEGAL SUFFIX FORMS IN THE PROTOTYPE AND RETURN
;
SUFFIX:
	MOV	HSTART,R0	;GET POINTER TO LAST CHAR.
	JSR	PC,ALPH		;IS CHARACTER ALPHABETIC?
	BNE	SUF08		;NO, EXIT
	JSR	PC,MAPLOW	;MAP CHAR. TO LOWER CASE
	SUB	#A,R2		;GET TABLE OFFSET
	ASL	R2		;CONVERT TO WORD INDEX
	MOV	SUFTAB(R2),-(SP) ;GET ADDRESS OF SUFFIX DICTIONARY
	BEQ	SUF07		;EXIT IF NO DICTIONARY ENTRY
SUF01:
	MOV	HSTART,R0	;ADDRESS OF LAST CHARACTER
	MOV	(SP),R1		;ADDRESS OF SUFFIX DICTIONARY
	JSR	PC,RDSUF	;GET A SUFFIX DICT. ENTRY
	MOVB	(R1),R3		;GET ENTRY LENGTH
	BEQ	SUF07		;END OF DICTIONARY
	BIC	#-17-1,R3	;CLEAR FLAG BITS
	ADD	R3,(SP)		;ADDRESS OF NEXT ENTRY
	ADD	R1,R3		;ADDRESS OF END OF ENTRY
SUF02:
	MOVB	-(R3),R2	;GET A CHARACTER
	CMP	R3,R1		;ALL DONE?
	BLE	SUF03		;YES,GOT IT
	BIC	#-177-1,R2	;MASK EXTRA FLAG BITS
	MOV	R2,-(SP)	;SAVE DICTIONARY CHARACTER
	MOVB	-(R0),R2	;GET SOURCE CHARACTER
	JSR	PC,MAPLOW	;MAP TO LOWER CASE
	CMP	R2,(SP)+	;DO WE HAVE A MATCH?
	BNE	SUF01		;NO, GO GET NEXT SUFFIX FORM
	BR	SUF02		;KEEP CHECKING
SUF03:
	MOV	HSTART,R0	;GET ADDRESS OF LAST SOURCE CHAR.
	TST	(SP)+		;DISCARD DIRECTORY ADDRESS
	MOVB	(R1),R3		;GET LENGTH OF SUFFIX FORM
	BIC	#-17-1,R3	;MASK OFF FLAG BITS
	ADD	R1,R3		;AND
	BITB	#200,(R1)+	;LOOK FOR THE HYPHEN
	BNE	SUF05		;FOUND IT!!
SUF04:
	DEC	R0		;FINE THE
	CMP	R3,R1		;LAST HYPHEN
	BLE	SUF06		;IN THE PROTOTYPE
	TSTB	-(R3)		;AND POSITION SOURCE
	BPL	SUF04		;POINTER ACCORDINGLY
SUF05:
	MOV	R0,HSTART	;SET POINTER TO LAST HYPHEN
	DEC	HSTART		;MINUS ONE
	BITB	#100,-1(R1)	;ONLY DO HYPHEN IF VOWEL PRECEDES?
	BNE	SUF04		;NO, KEEP GOING
	JSR	R5,CHECKVOW	;ANY VOWELS PRECEDING THIS CHAR?
	BNE	SUF08		;NO, EXIT
	BISB	#200,(R0)	;SET THE HYPHEN MARK
	BR	SUF04		;AND RE-LOOP
SUF06:
	BITB	#40,-(R1)	;SHOULD WE LOOK FOR MORE TO CHOP?
	BEQ	SUFFIX		;YES
	BR	SUF08		;NO, EXIT
SUF07:
	TST	(SP)+
SUF08:
	RTS	R5

;
;GET A SUFFIX DICTIONARY ENTRY
;	UPON RETURN, R1 HAS ADDRESS OF SUFFIX
;
RDSUF:
	MOV	R0,-(SP)
	MOV	4(SP),R1	;ADDRESS OF SUFF. DICT. ENTRY
	MOVB	@R1,R0		;GET CHARACTER IN ENTRY
	MOV	#SUFB,R2
	MOVB	R0,(R2)+	;STORE THE CHARACTER
	MOV	R0,R3
	BIC	#-17-1,R3	;GET COUNT OF CHARS. NEEDED
RDS01:
	DEC	R3		;LOOP UNTIL COMPLETE ENTRY
	BLT	RDS02		; IS STORED
	INC	R1
	MOVB	@R1,R0
	MOVB	R0,(R2)+
	BR	RDS01
RDS02:
	MOV	#SUFB,R1	;GET ADDRESS OF SUFFIX ENTRY
	MOV	(SP)+,R0
	RTS	PC		;AND RETURN


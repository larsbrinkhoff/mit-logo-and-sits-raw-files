GUNE:	ERROR+IFN
	CMP	B,NUSERS
	BGE	GUNE
	MUL #LUBLK,B
	MOV B,U
	CLR	FLSRES(U)
	CLR	FLSADR(U)
	MOV	#GOODBYE,UPC(U)
	MOV PC,BRAKE(U)

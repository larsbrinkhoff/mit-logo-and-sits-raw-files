WORD=1
POINT=2
BCNT=3
BITCNT=4
WORDP=5
BIT=6

START:	.OPEN [SIXBIT /  !TTY/]
	.VALUE
	MOVEI WORDP,WORDS
LOOP:	MOVEI BCNT,2
	MOVE POINT,[200100,,WORD]
	MOVE WORD,(WORDP)
	.IOT [15]
	.IOT [12]
PRINT:	MOVEI BITCNT,10
BPRINT:	ILDB BIT,POINT
	MOVEI BIT,60(BIT)
	.IOT BIT
	SOJG BITCNT,PRINT
	.IOT [40]
	SOJG BCNT,BPRINT
	AOJA WORDP,LOOP

WORDS:	BLOCK 50

	END START
